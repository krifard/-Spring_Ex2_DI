<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
          http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean class="com.spring.Location" id="l1">
        <property name="area" value="Jankipuram"></property>
        <property name="City" value="Lucknow"></property>
    </bean>
    <bean class="com.spring.Location" id="l2" primary="true">
        <property name="area" value="indranagar"></property>
        <property name="City" value="Bangalore"></property>
    </bean>
    <bean class="com.spring.Location" id="l3">
        <property name="area" value="HKV"></property>
        <property name="City" value="New Delhi"></property>
    </bean>
</beans>
