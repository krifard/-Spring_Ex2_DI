<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
          http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean class="com.spring.Mentor" id="m1">
        <property name="name" value="Vishal"></property>
        <property name="age" value="23"></property>
        <property name="mentorId" value="1"></property>
    </bean>
    <bean class="com.spring.Mentor" id="m2">
        <property name="name" value="Mohit"></property>
        <property name="age" value="26"></property>
        <property name="mentorId" value="2"></property>
    </bean>
    <bean class="com.spring.Mentor" id="m3">
        <property name="name" value="Rahul"></property>
        <property name="age" value="25"></property>
        <property name="mentorId" value="3"></property>
    </bean>

</beans>
